      },
      "source": [
        "City=df.groupby('City').apply(lambda x:x[\"Year\"].unique()).to_frame().reset_index()\n",
        "\n",
        "City.columns=[\"City\",\"Years\"]\n",
        "City[\"Occurence\"]=[len(c) for c in City['Years']]\n",
